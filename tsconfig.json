{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es2016",

    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    // "strict": true,
    "skipLibCheck": true,

    "lib": ["es5", "es6", "dom"],
